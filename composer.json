{
    "name": "spiderpan/doc-gpt",
    "description": "PHP library that for document GPT",
    "type": "library",
    "license": "MIT",
    "require": {
        "openai-php/client": "^0.8.1 || ^0.10.0 || ^0.14.0",
        "symfony/http-client": "^7.0",
        "nyholm/psr7": "^1.8",
        "guzzlehttp/guzzle": "^7.8",
        "pgvector/pgvector": "^0.1.4 || ^0.2.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "fakerphp/faker": "^1.23",
        "squizlabs/php_codesniffer": "*",
        "friendsofphp/php-cs-fixer": "^3.48"
    },
    "autoload": {
        "psr-4": {
            "Pan\\DocGpt\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Banglanfeng Pan",
            "email": "panbanglanfeng@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
